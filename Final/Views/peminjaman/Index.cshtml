@model IEnumerable<Final.Models.tabel_peminjaman>

@{
    ViewBag.Title = "Index";
}

<div class="row">
    <div class="box">
        <div class="col-lg-12">
            <h2>Index</h2>

            <p>
                @Html.ActionLink("Create New", "Create")
            </p>
            <table class="table">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.tanggal_peminjaman)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.tanggal_harus_kembali)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.status)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.tabel_buku.nama_buku)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.tabel_pegawai.nama_pegawai)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.tabel_peminjam.nama_peminjam)
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.tanggal_peminjaman)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.tanggal_harus_kembali)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.status)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.tabel_buku.nama_buku)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.tabel_pegawai.nama_pegawai)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.tabel_peminjam.nama_peminjam)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.id_peminjaman }) |
                            @Html.ActionLink("Details", "Details", new { id = item.id_peminjaman }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.id_peminjaman })
                        </td>
                    </tr>
                }

            </table>
        </div>
    </div>
</div>